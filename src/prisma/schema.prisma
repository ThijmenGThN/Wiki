generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl"]
}

datasource db {
  provider = "postgresql"
  url      = env("PRISMA_CONNECTOR")
}

model Category {
  name String @id

  items Item[]
}

model Item {
  name String @id

  content String

  categoryName String
  category     Category @relation(fields: [categoryName], references: [name])
}
